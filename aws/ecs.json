{
    "family": "my-ecs-task",
    "taskRoleArn": "",
    "executionRoleArn": "",
    "networkMode": "bridge",
    "containerDefinitions": [
        {
            "name": "container-name",
            "image": "",
            "repositoryCredentials": {
                "credentialsParameter": ""
            },
            "cpu": 1,
            "memory": 256,
            "startTimeout": 2,
            "stopTimeout": 2,
            "disableNetworking": true,
            "privileged": true,
            "readonlyRootFilesystem": true,
            "interactive": true,
            "pseudoTerminal": true
        }
    ],
    "volumes": [
        {
            "name": "my-volume",
            "dockerVolumeConfiguration": {
                "scope": "task",
                "autoprovision": true
            }
        }
    ],
    "requiresCompatibilities": [
        "EC2"
    ],
    "pidMode": "host",
    "ipcMode": "host",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "WINDOWS_SERVER_2019_FULL"
    }
}
