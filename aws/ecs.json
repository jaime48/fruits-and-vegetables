{
    "family": "my-ecs-task",
    "taskRoleArn": "",
    "executionRoleArn": "",
    "networkMode": "bridge",
    "containerDefinitions": [
        {
            "name": "container-name",
            "image": "",
            "repositoryCredentials": {
                "credentialsParameter": ""
            },
            "cpu": 1,
            "memory": 256,
            "startTimeout": 0,
            "stopTimeout": 0,
            "disableNetworking": true,
            "privileged": true,
            "readonlyRootFilesystem": true,
            "interactive": true,
            "pseudoTerminal": true,
            "ulimits": [
                {
                    "name": "core",
                    "softLimit": 0,
                    "hardLimit": 0
                }
            ]
        }
    ],
    "volumes": [
        {
            "name": "my-volume",
            "dockerVolumeConfiguration": {
                "scope": "task",
                "autoprovision": true,
            }
        }
    ],
    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "1",
    "memory": "256",
    "pidMode": "host",
    "ipcMode": "host",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "WINDOWS_SERVER_2019_FULL"
    }
}
